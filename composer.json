{
	"name": "consistence/consistence-jms-serializer-symfony",
	"description": "Symfony Bundle integrating Consistence library with JMS Serializer",
	"license": "MIT",
	"authors": [
		{
			"name": "Va≈°ek Purchart",
			"email": "me@vasekpurchart.cz",
			"homepage": "http://vasekpurchart.cz"
		}
	],
	"require": {
		"php": "~7.2",
		"consistence/consistence-jms-serializer": "~1.0",
		"jms/serializer-bundle": "~1.0|~2.0",
		"symfony/config": "~3.0",
		"symfony/dependency-injection": "~3.0",
		"symfony/http-kernel": "~3.0",
		"symfony/yaml": "~3.0"
	},
	"require-dev": {
		"consistence/coding-standard": "3.5",
		"jakub-onderka/php-console-highlighter": "0.3.2",
		"jakub-onderka/php-parallel-lint": "1.0",
		"matthiasnoback/symfony-dependency-injection-test": "2.3.1",
		"phing/phing": "2.16.1",
		"phpunit/phpunit": "6.5.12"
	},
	"autoload": {
		"psr-4": {
			"Consistence\\JmsSerializer\\SymfonyBundle\\": "src"
		},
		"classmap": [ "src" ]
	},
	"autoload-dev": {
		"psr-4": {
			"Consistence\\JmsSerializer\\SymfonyBundle\\": "tests"
		},
		"classmap": [ "tests" ]
	},
	"config": {
		"bin-dir": "bin"
	}
}
